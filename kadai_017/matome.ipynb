        "from os import name\n",
        "class Human:\n",
        "  def __init__(self,name:str, age:int):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def check_adult(self):\n",
        "    if self.age >= 20:\n",
        "      print(\"adult\")\n",
        "    else:\n",
        "      print(\"no_adult\")\n",
        "\n",
        "human = Human(\"侍　太郎\",36)\n",
        "human.check_adult()"
